<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.admin.test">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.CAPTURE_VIDEO_OUTPUT"/>

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recyclerview.RecyActivity">
        </activity>
        <activity android:name=".chart.ChartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".other.OtherActivity">
        </activity>
        <activity android:name=".camera.CameraActivity">
        </activity>
        <activity android:name=".LauncherActivity">

        </activity>
        <activity android:name=".expandable.MultisActivity"/>
        <activity
            android:name=".recyclerview.excel.ChannelGuideActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".BaseActivity"/>
        <activity android:name=".recyclerview.other.ListActivity"/>
        <activity android:name=".gradient.QRGradientActivity">
        </activity>
    </application>

</manifest>